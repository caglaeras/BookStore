@model List<BookStore.Models.Category>

    <ul class="nav flex-column">
        @foreach (var category in Model)
        {
        <li class="nav-item">
            <a class="nav-link" asp-controller="Categories" asp-action="Details" asp-route-id="@category.Id">
                @category.Name
            </a>
        </li>
        }
    </ul>
